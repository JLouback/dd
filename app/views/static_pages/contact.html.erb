<% provide(:title, 'Contato') %>
<h1>Contato</h1>
<div class="row">
	<div id="contact-form">
	<form id="contact">
		<fieldset class="form-group">
			<label for="contact-name">Nome</label>
			<input type="text" class="form-control" id="contact-name"><br>
			<label for="contact-email">Email</label>
			<input type="email" class="form-control" id="contact-email"><br>
			<label for="contact-message">Mensagem</label>
			<textarea class="form-control" id="contact-message" rows="5"></textarea>
			<button type="submit" id="contact-submit" class="btn">Enviar</button>
		</fieldset>
	</form>
	</div>
	<div id="contact-result">
	</div>
</div>

<script src="js/bootstrap.min.js"></script>
<script src="js/contact.js"></script>